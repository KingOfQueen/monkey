		SELECT * FROM game_role WHERE FLAG =1 
		<isNotEmpty prepend="AND" property="level">
			level BETWEEN #level#-5 AND #level#+3
		</isNotEmpty>
		<isNotEmpty prepend="AND" property="roleline">
			roleline = #roleline#
		</isNotEmpty>
		<isNotEmpty prepend="AND" property="id">
			id != #id#
		</isNotEmpty>
	</select>
	<select id="getRoleByLevels" parameterClass="java.util.HashMap"
		resultMap="listResult" >
		SELECT * FROM game_role WHERE FLAG =1 
		<isNotEmpty prepend="AND" property="level">
			level BETWEEN #level#-5 AND #level#+3
		</isNotEmpty>
		<isNotEmpty prepend="AND" property="id">
			id != #id#
		</isNotEmpty>
		<isNotEmpty prepend="AND" property="id">
			serverid = #serverId#
		</isNotEmpty>
		limit 50
	</select>
	<select id="findRoleByName" parameterClass="String"
		resultClass="GameRoleDetail">
		SELECT * FROM game_role WHERE FLAG =1 AND name =#name#
	</select>
</sqlMap>